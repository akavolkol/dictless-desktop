    <property name="title">Dictless</property>
    <property name="default_height">640</property>
    <property name="default_width">480</property>
    <child>
      <object class="GtkBox" id="app_container">
        <style>
          <class name="container"/>
        </style>
      </object>
    </child>
  </object>
</interface>